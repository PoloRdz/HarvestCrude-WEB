<?xml version="1.0" encoding="UTF-8"?>
<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">

	<url-mapping id="login">
		<pattern value="/login" />
		<view-id value="/index.xhtml" />
	</url-mapping>
	
	<url-mapping id="home">
		<pattern value="/home" />
		<view-id value="/private/home.xhtml" />
	</url-mapping>
	
	<url-mapping id="dispatch">
		<pattern value="/dispatch/#{dispatch}"/>
		<view-id value="/private/dispatch/dispatch.xhtml"/>
	</url-mapping>
	
	<url-mapping id="search-dispatch">
		<pattern value="/search/dispatch"/>
		<view-id value="/private/dispatch/search-dispatch.xhtml"/>
	</url-mapping>
	
	<url-mapping id="order">
		<pattern value="/order/#{order}"/>
		<view-id value="/private/order/order.xhtml"/>
	</url-mapping>
	
	<url-mapping id="split-load">
		<pattern value="/split-load/#{order}"/>
		<view-id value="/private/order/split-load.xhtml"/>
	</url-mapping>

</pretty-config>